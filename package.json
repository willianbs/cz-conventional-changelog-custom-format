{
  "name": "cz-conventional-changelog-custom-format",
  "version": "1.0.0",
  "description": "Prompts for conventional changelog standard with an extra step to insert Jira ID and other tweaks.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha *.test.js",
    "format": "prettier --write *.js"
  },
  "homepage": "https://github.com/willianbs/cz-conventional-changelog-custom-format",
  "repository": {
    "type": "git",
    "url": "https://github.com/willianbs/cz-conventional-changelog-custom-format.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "index.js",
    "engine.js"
  ],
  "author": "Will Silveira <will@silveira.works>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "commitizen": "^4.2.4",
    "conventional-commit-types": "^3.0.0",
    "git-branch": "^1.0.0",
    "lodash.map": "^4.5.1",
    "longest": "^2.0.1",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cosmiconfig": "^5.2.1",
    "mocha": "^10.7.3",
    "mock-require": "^3.0.3",
    "prettier": "^1.15.3",
    "semantic-release": "^22.0.12",
    "semver": "^6.2.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  }
}
